{
    "name": "falco442/lumen-treeview",
    "description": "Something to help manage tree data structures",
    "homepage": "https://falco442.github.io/lumen-treeview/",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "tree",
        "treeview",
        "multilevel"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/lumen-framework": "^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "falco442\\": "lib",
            "falco442\\Tests\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "falco442\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
